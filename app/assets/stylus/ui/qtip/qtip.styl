.qtip
    position absolute
    left -28000px
    top -28000px
    display none
    max-width 280px
    min-width 50px
    font-size 10.5px
    line-height 12px
    direction ltr
    box-shadow none
    padding 0

.qtip-content
    position relative
    padding 5px 5px
    overflow hidden
    text-align left
    word-wrap break-word

.qtip-titlebar
    position relative
    padding 5px 35px 5px 10px
    overflow hidden
    border-width 0 0 1px
    font-weight bold
    & + .qtip-content
        borderwidth 0 !important
    .qtip-close
        right 4px
        top 50%
        margin-top -9px

.qtip-close
    position absolute
    right -9px
    top -9px
    z-index 11
    cursor pointer
    outline medium none
    border 1px solid transparent

*
    html
        .qtip-titlebar
            .qtip-close
                top 16px

.qtip-titlebar .ui-icon,
		.qtip-icon .ui-icon
    display block
    text-indent -1000em
    direction ltr

.qtip-icon, .qtip-icon .ui-icon
    border-radius 3px
    border-radius 3px
    text-decoration none

.qtip-icon
    .ui-icon
        width 18px
        height 14px
        line-height 14px
        text-align center
        text-indent 0
        font normal bold 10px/13px Tahoma,sans-serif
        color inherit
        background transparent none no-repeat -100em -100em

    .qtip-default
        border 1px solid #F1D031
        background-color #FFFFA3
        color #555

.qtip-default
    .qtip-titlebar
        background-color #FFEF93
        .qtip-close
            border-color #AAA
            color #111
    .qtip-icon
        border-color #CCC
        background #F1F1F1
        color #777

.qtip-shadow
    box-shadow 1px 1px 3px 1px rgba(0, 0, 0, 0.15)

.qtip-rounded,
.qtip-tipsy,
.qtip-bootstrap
    border-radius 5px
    border-radius 5px

.qtip-rounded
    .qtip-titlebar
        border-radius 4px 4px 0 0
        border-radius 4px 4px 0 0

.qtip-youtube
    border-radius 2px
    border-radius 2px
    box-shadow 0 0 3px #333
    box-shadow 0 0 3px #333
    color white
    border 0 solid transparent
    background #4A4A4A
    .qtip-titlebar
        background-color #4A4A4A
        background-color rgba(0,0,0,0)
        .ui-state-hover
            border-color #303030
    .qtip-content
        padding .75em
        font 12px arial,sans-serif
    .qtip-icon
        border-color #222

.qtip-jtools
    background #232323
    border 2px solid rgba(241,241,241,1)
    border-radius 2px
    border-radius 2px
    box-shadow 0 0 12px #333
    .qtip-titlebar
        background-color transparent

        .ui-state-hover
            border-color #333

    .qtip-icon
        border-color #555

.qtip-jtools .qtip-titlebar,
	.qtip-jtools .qtip-content
    background transparent
    color white
    border 0 dashed transparent

.qtip-cluetip
    box-shadow 4px 4px 5px rgba(0, 0, 0, 0.4)
    background-color #D9D9C2
    color #111
    border 0 dashed transparent
    .qtip-titlebar
        background-color #87876A
        color white
        border 0 dashed transparent
        .ui-state-hover
            border-color #696952
            color #696952
    .qtip-icon
        border-color #808064

.qtip-tipsy
    background black
    background rgba(0, 0, 0, .87)
    color white
    border 0 solid transparent
    font-size 11px
    font-family 'Lucida Grande', sans-serif
    font-weight bold
    line-height 16px
    text-shadow 0 1px black
    .qtip-titlebar
        padding 6px 35px 0 10px
        background-color transparent
        .ui-state-hover
            border-color #303030
    .qtip-content
        padding 6px 10px
    .qtip-icon
        border-color #222
        text-shadow none

.qtip-tipped
    border 3px solid #959FA9
    border-radius 3px
    border-radius 3px
    background-color #F9F9F9
    color #454545
    font-weight normal
    font-family serif
    .qtip-titlebar
        borderwidth 0
        color white
        background #3A79B8
    .qtip-icon
        border 2px solid #285589
        background #285589
        .ui-icon
            background-color #FBFBFB
            color #555

.qtip
    font-size 13px
    line-height 20px
    color #333333
    padding 1px
    background-color #EFEFEF
    box-shadow 3px 3px 0px rgba(0, 0, 0, 0.2)
    background-clip padding-box
    background-clip padding
    background-clip padding-box
    &:after
        display block
        position absolute
        content ''
        bottom -5px
        left 50%
        size 12px
        transform rotate(45deg)
        background-color #EFEFEF
        box-shadow 3px 1px 0px rgba(0, 0, 0, 0.2)
    .qtip-titlebar
        padding 8px 14px
        margin 0
        font-size 14px
        font-weight normal
        line-height 18px
        background-color #f7f7f7
        border-bottom 1px solid #ebebeb
        border-radius 5px 5px 0 0
        .qtip-close
            right 11px
            top 45%
            border-style none
    .qtip-content
        padding 5px 9px
    .qtip-icon
        background transparent
        .ui-icon
            width auto
            height auto
            float right
            font-size 20px
            font-weight bold
            line-height 18px
            color #000000
            text-shadow 0 1px 0 #ffffff
            opacity 0.2
            &:hover
                color #000000
                text-decoration none
                cursor pointer
                opacity 0.4
